{
  "family": "jetcvnft-api-task",
  "executionRoleArn": "arn:aws:iam::480038061743:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "jetcvnft-api",
      "image": "480038061743.dkr.ecr.eu-south-1.amazonaws.com/jetcvnft-api:latest",
      "portMappings": [
        { "containerPort": 4000, "protocol": "tcp" }
      ],
      "essential": true
    }
  ]
}
