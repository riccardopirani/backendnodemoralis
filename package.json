{
  "name": "moralis-server",
  "version": "1.0.3",
  "description": "Moralis server for handling file uploads and interactions with a smart contract",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prepare": "husky install",
    "deploy-aws": "bash script/deploy.sh",
    "deploy-aws2": "bash script/deploy2.sh",
    "docker-deploy": "bash script/docker-deploy.sh",
    "docker-quick": "bash script/docker-quick.sh",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "test": "jest --detectOpenHandles",
    "test:server": "node test-server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@lighthouse-web3/sdk": "^0.4.0",
    "@prisma/client": "^6.13.0",
    "@storacha/client": "^1.6.0",
    "axios": "^1.11.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "fetch-blob": "^3.2.0",
    "files-from-path": "^1.1.4",
    "form-data": "^4.0.4",
    "moralis": "^2.27.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "web3.storage": "^4.5.5",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "open": "^10.2.0",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nft",
    "blockchain",
    "ethereum",
    "polygon",
    "smart-contract",
    "api",
    "express",
    "prisma"
  ],
  "author": "JetCV Team",
  "license": "MIT"
}
